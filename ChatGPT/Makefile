# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=chatgpt
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),chatgpt)
  EchoSrvGPT_config = chatgpt

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := EchoSrvGPT

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

EchoSrvGPT:
ifneq (,$(EchoSrvGPT_config))
	@echo "==== Building EchoSrvGPT ($(EchoSrvGPT_config)) ===="
	@${MAKE} --no-print-directory -C . -f EchoSrvGPT.make config=$(EchoSrvGPT_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f EchoSrvGPT.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  chatgpt"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   EchoSrvGPT"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"