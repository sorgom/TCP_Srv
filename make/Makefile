# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=prod
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),prod)
  TCP_Srv_Echo_config = prod

else ifeq ($(config),verbose)
  TCP_Srv_Echo_config = verbose

else ifeq ($(config),vsmall)
  TCP_Srv_Echo_config = vsmall

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := TCP_Srv_Echo

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

TCP_Srv_Echo:
ifneq (,$(TCP_Srv_Echo_config))
	@echo "==== Building TCP_Srv_Echo ($(TCP_Srv_Echo_config)) ===="
	@${MAKE} --no-print-directory -C . -f TCP_Srv_Echo.make config=$(TCP_Srv_Echo_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f TCP_Srv_Echo.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  prod"
	@echo "  verbose"
	@echo "  vsmall"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   TCP_Srv_Echo"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"